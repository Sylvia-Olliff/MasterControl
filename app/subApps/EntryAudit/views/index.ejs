    <body>
        <!--[if lt IE 8]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->
    <nav class="navbar navbar-fixed-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand text-white" href="#"><strong>File Review</strong></a>
          <div id="fileData" class="navbar-brand text-white"></div>
          <div id="entryData" class="navbar-brand text-white"></div>

        </div>
        <div id="navbar" class="navbar-collapse collapse">
        <img src="dist/img/footerLogo.png" align="right" width="160px" height="50px" style="padding-top: 6px;">
        </div><!--/.navbar-collapse -->
      </div>
    </nav>
    <div class="container">
      <hr>
      <br>
        <div class="alert alert-warning">
          <p class="text-center"><strong>NOTICE: You must Non-ABI the file before Data will show here!</strong></p>
        </div>
        <div class="row text-center flex">
            <div class="col-md-2 text-left">
              <label for="office" class="text-primary">Office:</label>
              <select id="office" name="office" class="">
                <option value="604">Atl</option>
                <option value="601" selected>Chs</option>
                <option value="605">Col</option>
                <option value="602">Gre</option>
                <option value="701">Hou</option>
                <option value="608">Nor</option>
              </select>
            </div>
            <div class="col-md-3">
              <label for="fileNum" class="text-primary">File: </label>
              <input id="fileNum" type="number" name="fileNum" placeholder="number" max="999999" />
            </div>
            <div class="col-md-2">
              <button id="all" name="all" class="btn btn-raised btn-primary">View All</button>
            </div>
            <div class="col-md-2">
              <label for="scrollInv" class="text-primary">Invoice: </label>
              <select id="scrollInv" name="scrollInv"></select>
            </div>
            <div class="col-md-2">
              <label for="scrollLin" class="text-primary">Line: </label>
              <select id="scrollLin" name="scrollLin"></select>
            </div>
            <div class="col-md-1">
              <button id="searchGo" name="searchGo" class="btn btn-raised btn-primary">GO!</button>
            </div>
          </div>
    </div>
    <div id="content">
      <hr>
      <div class="content">
        <div class="pane container" id="viewAll"></div>
      </div>
    </div>
    <div class="container">
      <hr>
      <hr>
    </div> <!-- /container -->  
    </body>
</html>
