          <link rel="stylesheet" href="css/bootstrap.css">
          <link rel="stylesheet" href="css/main.css">
          <link rel="stylesheet" href="css/jquery-ui.css">

          <link rel="stylesheet" type="text/css" href="css/bootstrap-material-design.css">
          <link rel="stylesheet" type="text/css" href="css/ripples.css">

        <% } else { %>
          <link rel="stylesheet" href="css/main.min.css">
        <% } %>
        <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap3-dialog/1.34.7/css/bootstrap-dialog.min.css">

        <script type="text/javascript" src="js/jquery-2.2.4.min.js"></script>
        <script type="text/javascript" src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
        <script defer src="https://code.getmdl.io/1.2.1/material.min.js"></script>
        <script type="text/javascript" src="js/bootstrap.min.js"></script>
        <script type="text/javascript" src="js/material.min.js"></script>
        <script type="text/javascript" src="js/ripples.min.js"></script>
        <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap3-dialog/1.34.7/js/bootstrap-dialog.min.js"></script>
        <script type="text/javascript" src="js/moment.min.js"></script>
        <script type="text/javascript" src="js/main.js"></script>
    </head>
    <body>
        <!--[if lt IE 8]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->
    <nav class="navbar navbar-fixed-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand text-white" href="#"><strong>Program Docs Interface</strong></a>
          <div id="fileData" class="navbar-brand text-white"></div>
          <div id="entryData" class="navbar-brand text-white"></div>

        </div>
        <div id="navbar" class="navbar-collapse collapse">
        <img src="img/footerLogo.png" align="right" width="160px" height="50px" style="padding-top: 6px;">
        </div><!--/.navbar-collapse -->
      </div>
    </nav>
    <div class="container">
        <hr><br>
        <div class="alert alert-info" hidden>
          <p class="text-center"><strong>Link to help docs for the use of this interface</strong></p>
        </div>
        <br>
        <div class="text-right">
            <button class="btn btn-raised btn-success" id="submit" name="submit">SUBMIT</button>
        </div>
        <br>
        <div class="row">
            <div class="well">
                <form class="form" id="pgmDoc">
                   <fieldset>
                       <legend>Program Header Documentation</legend>
                       <div class="form-group">
                           <label for="pgmName" class="col-md-1 control-label text-right">
                                <strong class="text-info">Program Name: </strong>
                           </label>
                           <div class="col-md-2">
                               <input type="text" name="pgmName" id="pgmName" class="form-control" data-toggle="tooltip" data-placement="top" title="" data-original-title="This is the name of your program">
                           </div>
                           <label for="pgmType" class="col-md-1 control-label text-right">
                                <strong class="text-info">Program Type: </strong>
                           </label>
                           <div class="col-md-2">
                               <select id="pgmType" name="pgmType" class="form-control" data-toggle="tooltip" data-placement="top" title="" data-original-title="What kind of program is it?">
                                   <option value="RPGLE">RPGLE</option>
                                   <option value="SQLRPGLE">SQLRPGLE</option>
                                   <option value="CL">CL</option>
                                   <option value="CMD">CMD</option>
                                   <option value="REPORT">REPORT</option>
                                   <option value="VIEW">VIEW</option>
                                   <option value="WEB">WEB</option>
                               </select>
                           </div>
                           <label for="system" class="col-md-1 control-label text-right">
                                <strong class="text-info">System: </strong>
                           </label>
                           <div class="col-md-2">
                               <input type="text" name="system" id="system" class="form-control" data-toggle="tooltip" data-placement="top" title="" data-original-title="If this is part of a larger group of programs, what is that group? (ACE FDA for example)">
                           </div>
                           <label for="proDate" class="col-md-1 control-label text-right">
                                <strong class="text-info">Date in Production</strong>
                           </label>
                           <div class="col-md-2">
                               <input type="text" name="proDate" id="proDate" class="form-control">
                           </div>
                       </div>
                       <br>
                       <br>
                       <br>
                       <div class="progress">
                           <div class="progress-bar progress-bar-primary" style="width: 100%"></div>
                       </div>
                       <div class="form-group">
                           <label for="reqBy" class="col-md-1 control-label text-right">
                                <strong class="text-info">Requested By: </strong>
                           </label>
                           <div class="col-md-2">
                               <input type="text" name="reqBy" id="reqBy" class="form-control" data-toggle="tooltip" data-placement="top" title="" data-original-title="Who requested this program?">
                           </div>
                           <label for="department" class="col-md-1 control-label text-right">
                                <strong class="text-info">Department: </strong>
                           </label>
                           <div class="col-md-2">
                               <select id="department" name="department" class="form-control"  data-toggle="tooltip" data-placement="top" title="" data-original-title="Which department is this primarily for">
                                   <option value="ADMIN">ADMIN</option>
                                   <option value="DP">DP</option>
                                   <option value="DRAWBACK">DRAWBACK</option>
                                   <option value="ENTRY">ENTRY</option>
                                   <option value="EXPORT">EXPORT</option>
                                   <option value="IMPORT">IMPORT</option>
                                   <option value="RBNAL">RBNAL</option>
                               </select>
                           </div>
                           <label for="location" class="col-md-1 control-label text-right">
                               <strong class="text-info">Location: </strong>
                           </label>
                           <div class="col-md-2">
                               <input type="text" name="location" id="location" class="form-control" data-toggle="tooltip" data-placement="top" title="" data-original-title="If this is a Green-Screen which library is it in (source), if this is web, what is its absolute path?">
                           </div>
                           <label for="reqDate" class="col-md-1 control-label text-right">
                                <strong class="text-info">Date Requested: </strong>
                           </label>
                           <div class="col-md-2">
                               <input type="text" name="reqDate" id="reqDate" class="form-control">
                           </div>
                       </div>
                       <br>
                       <br>
                       <div class="form-group">
                           <label for="design" class="col-md-1 control-label text-right">
                                <strong class="text-info">Design Members: </strong>
                           </label>
                           <div class="col-md-2">
                               <input type="text" name="design" id="design" class="form-control">
                           </div>
                           <label for="calledBy" class="col-md-1 control-label text-right">
                                <strong class="text-info">Called By: </strong>
                           </label>
                           <div class="col-md-2">
                               <textarea class="form-control" id="calledBy" name="calledBy" rows="10" style="font-size: 9pt;"  data-toggle="tooltip" data-placement="top" title="" data-original-title="Which programs/menus/CLs call this program?">Comma separate each program name.</textarea>
                           </div>
                           <label for="calls" class="col-md-1 control-label text-right">
                                <strong class="text-info">Calls: </strong>
                           </label>
                           <div class="col-md-2">
                               <textarea class="form-control" id="calls" name="calls" rows="10" style="font-size: 9pt;"  data-toggle="tooltip" data-placement="top" title="" data-original-title="What programs does this one call?">Comma separate each program name.</textarea>
                           </div>
                           <label for="funcKeys" class="col-md-1 control-label text-right">
                                <strong class="text-info">Function Keys: </strong>
                           </label>
                           <div class="col-md-2">
                               <select id="funcKeys[]" name="funcKeys[]" class="form-control" data-toggle="tooltip" data-placement="top" title="" data-original-title="Hold down the Ctrl button and click on each function key your program uses. NOTE: make sure you release the Ctrl button first if you use the mouse wheel to scroll the list" multiple>
                                   <option  value="1">F1 - help           </option>
                                   <option  value="2">F2 - Fold           </option>
                                   <option  value="3">F3 - Exit           </option>
                                   <option  value="4">F4 - Prompt         </option>
                                   <option  value="5">F5 - Refresh        </option>
                                   <option  value="6">F6 - New/Add        </option>
                                   <option  value="7">F7 - Print          </option>
                                   <option  value="8">F8 - Define in Desc </option>
                                   <option  value="9">F9 - Email          </option>
                                   <option value="10">F10 - Additional options</option>
                                   <option value="11">F11 - Define in Desc</option>
                                   <option value="12">F12 - Return        </option>
                                   <option value="13">F13 - Save Data     </option>
                                   <option value="14">F14 - Define in Desc</option>
                                   <option value="15">F15 - Define in Desc</option>
                                   <option value="16">F16 - Define in Desc</option>
                                   <option value="17">F17 - Define in Desc</option>
                                   <option value="18">F18 - Define in Desc</option>
                                   <option value="19">F19 - Define in Desc</option>
                                   <option value="20">F20 - Define in Desc</option>
                                   <option value="21">F21 - Define in Desc</option>
                                   <option value="22">F22 - Define in Desc</option>
                                   <option value="23">F23 - Confirm       </option>
                                   <option value="24">F24 - Define in Desc</option>
                               </select>
                           </div>
                       </div>
                   </fieldset>
                   <br><br>
                   <div class="progress">
                       <div class="progress-bar progress-bar-primary" style="width: 100%"></div>
                   </div>
                   <hr>
                   <fieldset>
                       <legend>Program Details</legend>
                       <div class="form-group">
                           <label for="pgmTask" class="col-md-1 control-label text-right"">
                                <strong class="text-info">Task: </strong>
                           </label>
                           <div class="col-md-3">
                               <textarea class="form-control" id="pgmTask" name="pgmTask" rows="10" data-toggle="tooltip" data-placement="top" title="" data-original-title="Using real world language what is your program suppose to do? What problem does it solve?"></textarea>
                           </div>
                           <label for="pgmFiles" class="col-md-1 control-label text-right">
                                <strong class="text-info">Files Used: </strong>
                           </label>
                           <div class="col-md-3">
                               <textarea class="form-control" id="pgmFiles" name="pgmFiles" rows="10" data-toggle="tooltip" data-placement="top" title="" data-original-title="Which files does this program use? Seperate each by a comma"></textarea>
                           </div>
                           <label for="pgmDesc" class="col-md-1 control-label text-right">
                                <strong class="text-info">Description: </strong>
                           </label>
                           <div class="col-md-3">
                               <textarea class="form-control" id="pgmDesc" name="pgmDesc" rows="10" data-toggle="tooltip" data-placement="top" title="" data-original-title="Describe how your program goes about performing its task, this can be as high level or detailed as necessary"></textarea>
                           </div>
                       </div>
                   </fieldset>
                   <br><br>
                   <div class="progress">
                       <div class="progress-bar progress-bar-primary" style="width: 100%"></div>
                   </div>
                   <hr>
                   <div class="alert alert-warning">
                       <p class="text-center"><strong>Does this Program have seperate components or Subroutines?</strong></p>
                   </div>
                   <div id="content">
                           
                   </div>
                   <button id="addComponent" class="btn btn-default btn-xs"><i class="material-icons">note_add</i></button>
                   <label for="addComponent" class="control-label"><strong class="text-info">Add Component</strong></label>

                   <div id="submissionResponse" class="mdl-js-snackbar mdl-snackbar">
                      <div class="mdl-snackbar__text"></div>
                      <button type="button" class="mdl-snackbar__action"></button>
                   </div>
               </form>
           </div>
        </div>
    </div>
    </body>
</html>
